{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"standard",
		"plugin:react/jsx-runtime",
		"prettier",
		"eslint-config-prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/prop-types": "off",
		"react/react-in-jsx-scope": "off",
		"no-console": "error"
	}
}
